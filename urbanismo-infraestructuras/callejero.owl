        <dc:creator>Manuel Tobarra (Ayuntamiento de Albacete)</dc:creator>
        <dc:identifier>http://vocab.linkeddata.es/datosabiertos/def/urbanismo-infraestructuras/callejero</dc:identifier>
        <dc:creator>Oscar Corcho (Ontology Engineering Group, Universidad Politécnica de Madrid)</dc:creator>
        <dc:creator>María Jesús Fernández (Ayuntamiento de Zaragoza)</dc:creator>
        <dc:title xml:lang="es">Vocabulario para la representación de datos de un callejero</dc:title>
        <dc:creator>Víctor Morlán (Ayuntamiento de Zaragoza)</dc:creator>
        <cc:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
